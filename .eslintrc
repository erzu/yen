{
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {
    "curly": [2, "multi-line"],
    "consistent-return": 0,
    "quotes": [2, "single"],
    "semi": [2, "never"],
    "strict": [2, "global"],
    "no-underscore-dangle": 0,
    "no-use-before-define": [2, "nofunc"],
    "no-undef": 2,
    "no-unused-vars": 2
  }
}
